package cap5;

public class Contrutores {
	String fabricante;
	String modelo;
	int ano;
	
	Contrutores(){
		setFabricante(fabricante);
	}
	//fabricante
	String getFabricante() {
		return fabricante;
	}
	
	void setFabricante(String fabricante) {
		this.fabricante = fabricante;
	}	
	//modelo
	String getModelo() {
		return modelo;
	}
	
	void setModelo(String modelo) {
		this.modelo = modelo;
	}
	//ano
	int getAno() {
		return ano;
	}
	
	void setModelo(int ano) {
		this.ano = ano;
	}
	
}
