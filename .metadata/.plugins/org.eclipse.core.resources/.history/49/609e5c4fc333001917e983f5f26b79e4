<script type="text/javascript">
function mostraArray(){
	var cars = ["Uno","Gol","Monza","Tempra","Parati"];
	$dado +="<tr>";
	for( i = 0; i < cars.length; i++){
		$dado +="<td>cars[i]</td>";
		//alert(cars[i]);		
	}
	$dado +="<tr>";
	//alert("O numero de carros é: " + cars.count);
}
</script>

