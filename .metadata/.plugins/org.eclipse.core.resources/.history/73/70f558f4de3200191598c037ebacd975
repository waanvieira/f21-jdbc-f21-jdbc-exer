package cap11;

public class VariosExemplos {
	public static void main(String[] args) {
	//Treeset garante que a ordem dos elementos seja ascendente, tomando
		//por base sua ordem narutal - não permite duplicações
		
		System.out.println("--Com Treeset ------------------");
		
		Set<String> cursos - new TreeSet<>();
		
		cursos.add("PHP");
		cursos.add("PHP");
		cursos.add("Linux");
		cursos.add("Asp");
		cursos.add("Java");
		cursos.add("Delphi");
		
		for(String curso : cursos) {
			System.out.println("Curso: " + curso);
		}
		
		System.out.println(cursos);
	
	}
	
}
